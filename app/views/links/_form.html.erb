<%= render 'layouts/errors', :object => @link %>
<%= form_for(@link) do |f| %>
  <div class="form-group ">
    <%= f.label :name, "Title" %>
    <%= f.text_field :name %>
  </div>
  <div class="form-group">
    <%= f.label :url, "url http://" %>
    <%= f.text_field :url %>
  </div>
  <%= f.submit(:class => "btn btn-primary") %>
<% end %>

